    <div class="row">
      <div class="col-md-4">
        <div class="home-devices">
          <h3><%= @content.section_1_title %></h3>
          <p><%= @content.section_1_first_paragraph %></p>
          <p><%= @content.section_1_second_paragraph %></p>
