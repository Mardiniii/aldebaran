<section class="carousel-section">
  <div id="carousel-example-generic" class="carousel carousel-razon slide" data-ride="carousel" data-interval="5000">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1"></li>
      <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <div class="container">
          <div class="row">
            <div class="col-md-6 col-sm-7">
              <div class="carousel-caption">
                <div class="carousel-text">
                  <h1 class="animated fadeInDownBig animation-delay-7 carousel-title"><%= @content.slide_1_title %></h1>
                  <h2 class="animated fadeInDownBig animation-delay-5  crousel-subtitle"><%= @content.slide_1_subtitle %></h2>
                  <ul class="list-unstyled carousel-list">
                    <li class="animated bounceInLeft animation-delay-11"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_1_text}".html_safe, @content.carousel_link_1, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-13"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_2_text}".html_safe, @content.carousel_link_2, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-15"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_3_text}".html_safe, @content.carousel_link_3, class: "white-text" %></li>
                  </ul>
                  <p class="animated fadeInDown animation-delay-8 text-center"><%= link_to "Buy Strategies", store_my_store_path, class: "btn btn-ar btn-primary btn-lg" %></p>
                </div>
              </div>
            </div>
            <div class="col-md-6 col-sm-5 hidden-xs carousel-img-wrap">
              <div class="carousel-img">
                <img src="<%= asset_path('demo/pre.png') %>" class="img-responsive animated bounceInUp animation-delay-3" alt="Image">
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="item">
        <div class="container">
          <div class="row">
            <div class="col-md-6 col-sm-8">
              <div class="carousel-caption">
                <div class="carousel-text">
                  <h1 class="animated fadeInDownBig animation-delay-7 carousel-title"><%= @content.slide_2_title %></h1>
                  <h2 class="animated fadeInDownBig animation-delay-5  crousel-subtitle"><%= @content.slide_2_subtitle %></h2>
                  <ul class="list-unstyled carousel-list">
                    <li class="animated bounceInLeft animation-delay-11"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_1_text}".html_safe, @content.carousel_link_1, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-13"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_2_text}".html_safe, @content.carousel_link_2, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-15"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_3_text}".html_safe, @content.carousel_link_3, class: "white-text" %></li>
                  </ul>
                  <p class="animated fadeInDown animation-delay-8 text-center"><%= link_to "Buy Strategies", store_my_store_path, class: "btn btn-ar btn-primary btn-lg" %></p>
                </div>
              </div>
            </div>
            <div class="col-md-6 col-sm-4 hidden-xs carousel-img-wrap">
              <div class="carousel-img">
                <img src="<%= asset_path('demo/pre2.png') %>" class="img-responsive animated bounceInUp animation-delay-3" alt="Image">
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="item">
        <div class="container">
          <div class="row">
            <div class="col-md-6 col-sm-8">
              <div class="carousel-caption">
                <div class="carousel-text">
                  <h1 class="animated fadeInDownBig animation-delay-7 carousel-title"><%= @content.slide_3_title %><%= @content.slide_3_title %></h1>
                  <h2 class="animated fadeInDownBig animation-delay-5  crousel-subtitle"><%= @content.slide_2_subtitle %></h2>
                  <ul class="list-unstyled carousel-list">
                    <li class="animated bounceInLeft animation-delay-11"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_1_text}".html_safe, @content.carousel_link_1, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-13"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_2_text}".html_safe, @content.carousel_link_2, class: "white-text" %></li>
                    <li class="animated bounceInLeft animation-delay-15"><%= link_to "<i class='fa fa-check'></i>#{@content.carousel_link_3_text}".html_safe, @content.carousel_link_3, class: "white-text" %></li>
                  </ul>
                  <p class="animated fadeInDown animation-delay-8 text-center"><%= link_to "Buy Strategies", store_my_store_path, class: "btn btn-ar btn-primary btn-lg" %></p>
                </div>
              </div>
            </div>
            <div class="col-md-6 col-sm-4 hidden-xs carousel-img-wrap">
              <div class="carousel-img">
                <img src="<%= asset_path('demo/pre2.png') %>" class="img-responsive animated bounceInUp animation-delay-3" alt="Image">
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
  </div>
</section> <!-- carousel -->

