<div class="container">
  <div class="row" style="margin-top: 30px;">
    <div class="col-md-7 text-center">
      <div class="title-logo animated fadeInDown animation-delay-5">aldebarantech</div>
    </div>
    <div class="col-md-5 text-center">
      <img src="http://i592.photobucket.com/albums/tt5/Mardini03/branch22_1.png" class="animated fadeInDown animation-delay-5" alt="">
      <!-- <div class="title-logo animated fadeInDown animation-delay-5">aldebarantech</div> -->
    </div>
    <div class="col-md-7 text-justify">
      <h2 class="right-line">Partnership</h2>
      <p><%= @content.partnership_first_paragraph %></p>
      <p><%= @content.partnership_second_paragraph %></p>
      <p><%= @content.partnership_third_paragraph %></p>
    </div>
    <div class="col-md-5">
      <h2 class="right-line">Our Approach</h2>
      <div class="panel-group" id="accordion">
        <div class="panel panel-default">
          <div class="panel-heading panel-heading-link">
            <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
              <i class="fa fa-lightbulb-o"></i> <%= @content.partnership_1_title %>
            </a>
          </div>
          <div id="collapseOne" class="panel-collapse collapse in">
            <div class="panel-body text-justify">
              <p><%= @content.partnership_1_first_paragraph %></p>
              <p><%= @content.partnership_1_second_paragraph %></p>
            </div>
          </div>
        </div>
        <div class="panel panel-default">
          <div class="panel-heading panel-heading-link">
            <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" class="collapsed">
              <i class="fa fa-desktop"></i> <%= @content.partnership_2_title %>
            </a>
          </div>
          <div id="collapseTwo" class="panel-collapse collapse">
            <div class="panel-body text-justify">
              <p><%= @content.partnership_2_first_paragraph %></p>
              <p><%= @content.partnership_2_second_paragraph %></p>
            </div>
          </div>
        </div>
        <div class="panel panel-default">
          <div class="panel-heading panel-heading-link">
            <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree" class="collapsed">
              <i class="fa fa-user"></i> <%= @content.partnership_3_title %>
            </a>
          </div>
          <div id="collapseThree" class="panel-collapse collapse">
            <div class="panel-body text-justify">
              <p><%= @content.partnership_3_first_paragraph %></p>
              <p><%= @content.partnership_3_second_paragraph %></p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div> <!-- row -->
</div> <!-- container -->

<div class="container">
  <div class="row">
    <div class="col-md-12 text-center">
      <img src="<%= @about_us_banner.image.url(:about_us) %>" class="img-responsive" alt="">
    </div>
  </div>
</div>